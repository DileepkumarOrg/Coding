

function person(first, last){
    this.firstName = first,
    this.lastName = last,
    this.grtFullName = function() {
        return this.firstName + " " + this.lastName
    }
}


const person1 = new person("Dileep", "Kumar");
const person2 = new person("Sandeep", "Kumar");
console.log(person1, person2);
console.log(person1.grtFullName());
console.log(person2.grtFullName());


